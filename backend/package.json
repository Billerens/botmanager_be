{
  "name": "botmanager-backend",
  "version": "1.0.0",
  "description": "BotManager Backend API",
  "author": "BotManager Team",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "yarn typeorm -- migration:generate -d src/database/data-source.ts",
    "migration:run": "yarn typeorm -- migration:run -d src/database/data-source.ts",
    "migration:revert": "yarn typeorm -- migration:revert -d src/database/data-source.ts",
    "migration:show": "yarn typeorm -- migration:show -d src/database/data-source.ts",
    "db:check": "yarn typeorm -- query \"SELECT 1\" -d src/database/data-source.ts",
    "start:with-migrations": "chmod +x scripts/start-with-migrations.sh && ./scripts/start-with-migrations.sh"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.908.0",
    "@aws-sdk/s3-request-presigner": "^3.908.0",
    "@nestjs/bull": "^10.0.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.2.8",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/throttler": "^5.0.1",
    "@nestjs/typeorm": "^10.0.0",
    "@nestjs/websockets": "^10.2.8",
    "@socket.io/redis-adapter": "^8.3.0",
    "@types/multer": "^2.0.0",
    "@types/sharp": "^0.31.1",
    "@types/speakeasy": "^2.0.10",
    "@types/uuid": "^7.0.8",
    "axios": "^1.12.2",
    "bcrypt": "^5.1.1",
    "bull": "^4.11.3",
    "bytez.js": "^1.1.18",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "express-rate-limit": "^6.10.0",
    "form-data": "^4.0.4",
    "helmet": "^7.0.0",
    "multer": "^2.0.2",
    "node-telegram-bot-api": "^0.64.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sharp": "^0.34.4",
    "socket.io": "^4.7.2",
    "speakeasy": "^2.0.0",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.17",
    "uuid": "^8.3.2",
    "winston": "^3.17.0",
    "winston-loki": "^6.1.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/node-telegram-bot-api": "^0.64.0",
    "@types/passport-jwt": "^3.0.9",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
